{
  "name": "sass-bootstrap-ts-webpack-boilerplate",
  "title": "Typescript Webpack Boilerplate",
  "description": "A starter template/boilerplate built with TypeScript/JavaScript, Babel, Webpack, SCSS, ESLint, and Pretieer.",
  "main": "",
  "url": "URL",
  "target": "web",
  "private": false,
  "license": "MIT",
  "version": "4.0.1",
  "copyright": "2013",
  "author": "Ali Bagheri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliBagheri2079/Sass-Bootstrap-TS-Webpack-Boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/AliBagheri2079/Sass-Bootstrap-TS-Webpack-Boilerplate/issues"
  },
  "homepage": "https://github.com/AliBagheri2079/Sass-Bootstrap-TS-Webpack-Boilerplate#readme",
  "scripts": {
    "build": "webpack --progress --config webpack.config.babel.js",
    "check-types": "tsc --project tsconfig.json --noEmit",
    "lint:all": "yarn lint:scripts && yarn check-types && yarn lint:styles",
    "lint:all:fix": "yarn lint:scripts:fix && yarn lint:styles:fix",
    "lint:scripts": "eslint --ignore-path .gitignore **/*.{js,jsx,json,ts,tsx}",
    "lint:scripts:fix": "yarn lint:scripts --fix",
    "lint:styles": "stylelint --ignore-path .gitignore **/*.css",
    "lint:styles:fix": "yarn lint:styles --fix",
    "prepare": "husky install",
    "serve": "cross-env NODE_ENV=development webpack serve --config webpack.config.babel.js",
    "check-size": "webpack --config webpack.config.babel.js --profile --json > webpack-stats.json && webpack-bundle-analyzer webpack-stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@types/bootstrap": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": ">=6",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": ">=13",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "purgecss-webpack-plugin": "^4.1.3",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "terser-webpack-plugin": "^5.3.5",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^v4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "SASS",
    "Bootstrap",
    "Babel",
    "ES6",
    "ESLint",
    "Prettier",
    "Stylelint",
    "PurgeCss",
    "JavaScript",
    "TypeScript",
    "Webpack",
    "Template",
    "Boilerplate"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1"
  }
}
